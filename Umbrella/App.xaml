            <x:String x:Key="CalendarTabIconFile">menu_calendar</x:String>
            <x:String x:Key="MessagesTabIconFile">messages_tab_icon</x:String>
            <x:String x:Key="MedalsTabIconFile">menu_medal</x:String>
            <x:String x:Key="CallBacksTabIconFile">appointments_tab_icon</x:String>
            <x:String x:Key="CamouBackgroundTabIconFile">camou_bg</x:String>
            <!-- Colors -->
            <Color x:FactoryMethod="FromHex"
                   x:Key="PrimaryColor">
                <x:Arguments>
                    <x:String>435742</x:String>
                </x:Arguments>
            </Color>
