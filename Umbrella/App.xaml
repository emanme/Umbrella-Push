            <Color x:FactoryMethod="FromHex"
                   x:Key="PageHeaderColor">
                <x:Arguments>
                    <x:String>D9D9D9</x:String>
                </x:Arguments>
            </Color>
            <!-- Fonts -->
            <x:Double x:Key="HeaderFontSize">18</x:Double>
            <x:Double x:Key="PageHeadersFontSize">22</x:Double>
            <x:Double x:Key="BackButtonFontSize">35</x:Double>
            <x:Double x:Key="LabelFontSize">16</x:Double>
            <x:Double x:Key="RankLabelSize">16</x:Double>
            <x:Double x:Key="BarcodeFontSize">12</x:Double>
            <x:Double x:Key="TextFontSize">12</x:Double>
            <x:Double x:Key="InputFontSize">14</x:Double>
            <x:Double x:Key="ButtonFontSize">12</x:Double>
            <x:Double x:Key="IconFontSize">30</x:Double>
            <x:Double x:Key="BarcodeNumberFontSize">25</x:Double>
            <x:Double x:Key="EarnedValueFontSize">22</x:Double>
            <x:Double x:Key="BulletIconFontSize">8</x:Double>
            <!--<OnPlatform x:Key="ButtonFontSize"
                        x:TypeArguments="x:Double"
                        iOS="8"
                        Android="12" />-->
            <!-- Dimensions -->
            <OnPlatform x:Key="ButtonsRowSpacing"
                        x:TypeArguments="x:Double"
                        iOS="8"
                        Android="0" />
            <OnPlatform x:Key="RowDefinitionHeight"
                        x:TypeArguments="GridLength"
                        iOS="50"
                        Android="58"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>