                    <x:String>8F0607</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="PrimaryLightColor">
                <x:Arguments>
                    <x:String>FA8D8D</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="AccentColor">
                <x:Arguments>
                    <x:String>363636</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="PrimaryTextColor">
                <x:Arguments>
                    <x:String>212121</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="SecondaryTextColor">
                <x:Arguments>
                    <x:String>757575</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="IconsColor">
                <x:Arguments>
                    <x:String>FFFFFF</x:String>
                </x:Arguments>
            </Color>
            <Color x:FactoryMethod="FromHex"
                   x:Key="DividerColor">
                <x:Arguments>
                    <x:String>BDBDBD</x:String>
                </x:Arguments>
            </Color>
