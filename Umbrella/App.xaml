<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Umbrella.App">
    <Application.Resources>
        <ResourceDictionary>
            <!-- Titles -->
            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="{DynamicResource backgroundColor}"/>
                  <Setter Property="BarTextColor" Value="{DynamicResource backTextColor}"/>
            </Style>
                
            <x:String x:Key="AppNameString">Umbrella</x:String>
            <x:String x:Key="LoginString">LOGIN</x:String>
            <x:String x:Key="HomeString">Home</x:String>
            <x:String x:Key="RankString">RANK</x:String>
            <x:String x:Key="MessagesString">MESSAGES</x:String>
            <x:String x:Key="RecruitsString">RECRUITS</x:String>
            <x:String x:Key="MedalString">MEDALS</x:String>
            <x:String x:Key="ComradesString">COMRADES</x:String>
            <x:String x:Key="ResourcesString">RESOURCES</x:String>
            <x:String x:Key="WeaponsString">WEAPONS</x:String>
            <x:String x:Key="CharityString">CHARITY</x:String>
            <x:String x:Key="TargetsString">TARGETS</x:String>
            <x:String x:Key="CalendarsString">CALENDAR</x:String>
            <x:String x:Key="CallBackString">CALL BACKS</x:String>
            <x:String x:Key="CustomerServicesString">Customer Services</x:String>
             <x:String x:Key="CustomersString">Customers</x:String>
            <x:String x:Key="ReferAndEarnString">REFER &amp; EARN</x:String>
            <x:String x:Key="YourRankString">YOUR RANK</x:String>
            <x:String x:Key="BackIconString">&lt;</x:String>
            <x:String x:Key="EarningsString">EARNINGS</x:String>

            <x:String x:Key="OtherString">Other</x:String>
             <x:String x:Key="YourNotesString">Your Notes</x:String>
            <x:String x:Key="AddNotesString">Add Notes</x:String>
              <x:String x:Key="AgentsNotesString">Agents Notes</x:String>

            
            <x:String x:Key="SuppliersString">Suppliers</x:String>
            <x:String x:Key="StaffString">Staff</x:String>
            <x:String x:Key="TopupString">Topup</x:String>
            <x:String x:Key="SettingsString">Settings</x:String>
            <x:String x:Key="NotificationsString">Notifications</x:String>
            <x:String x:Key="TermsAndConditionsString">Terms &amp; Conditions</x:String>
            <x:String x:Key="PrivacyPolicyString">Privacy Policy</x:String>
            <x:String x:Key="ContactUsString">Contact Us</x:String>
            
            <x:String x:Key="EnquiriesString">Enquiries</x:String>
            <x:String x:Key="AllEnquiriesString">All Enquiries</x:String>
            
            <x:String x:Key="AppointmentsString">Appointments</x:String>
            
            <!-- PAGE HEADER MARGINS -->
            <Style x:Key="PageHeaderMargins" TargetType="Label">
                <Setter Property="Margin" Value="100,8,0,0"/>
                <Setter Property="FontAttributes" Value="Bold"/>
            </Style>
            
            <!-- Login -->
            <x:String x:Key="UsernameString">Email Address / Username</x:String>
            <x:String x:Key="PasswordString">Password</x:String>
            <x:String x:Key="ForgotPasswordString">Forgot Password?</x:String>
            <x:String x:Key="EmailString">Email Address</x:String>
            <!-- Files -->
            <x:String x:Key="UmbrellaLogoFile">umbrella_army_logo</x:String>
            <x:String x:Key="TopupIconFile">topup_icon</x:String>
            <x:String x:Key="SettingsIconFile">settings_icon</x:String>
            <x:String x:Key="RecruitsIconFile">recruits</x:String>
            <x:String x:Key="ComradesIconFile">comrades</x:String>
            <x:String x:Key="ResourcesIconFile">resources</x:String>
            <x:String x:Key="WeaponsIconFile">weapons</x:String>
            <x:String x:Key="CharityIconFile">charity</x:String>
            <x:String x:Key="TargetsIconFile">targets</x:String>
            <x:String x:Key="LeadsNewIconFile">leads_new_icon</x:String>
            <x:String x:Key="CallBacksNewIconFile">menu_callbacks</x:String>
            <x:String x:Key="CustomerServicesIconFile">customer_services_icon</x:String>
            <x:String x:Key="OtherIconFile">other_icon</x:String>
            <x:String x:Key="SuppliersIconFile">suppliers_icon</x:String>
            <x:String x:Key="StaffIconFile">staff_icon</x:String>
            <x:String x:Key="HomeTabIconFile">home_tab_icon</x:String>
